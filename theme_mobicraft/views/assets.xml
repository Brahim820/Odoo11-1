<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- Assets Frontend -->
        <template id="asset_frontend" inherit_id="website.assets_frontend" name="Theme Mobicraft assets frontend">
            <xpath expr="." position="inside">
                <!-- Libraries -->
                <link href="/theme_mobicraft/static/src/css/owl.carousel.css" rel="stylesheet"/>
                <link href="/theme_mobicraft/static/src/css/unite-gallery.css" rel="stylesheet"/>
                <link href="/theme_mobicraft/static/src/skins/alexis/alexis.css" rel="stylesheet"/>
                <link href="/theme_mobicraft/static/src/css/ion.rangeSlider.css" rel="stylesheet"/>
                <link href="/theme_mobicraft/static/src/css/ion.rangeSlider.skinHTML5.css" rel="stylesheet"/>
                <link href="/theme_mobicraft/static/src/css/theme_mobicraft.css" rel="stylesheet"/>
                <script src="/theme_mobicraft/static/src/js/unitegallery.min.js" type="text/javascript"/>
                <script src="/theme_mobicraft/static/src/js/ug-theme-compact.js" type="text/javascript"/>
                <script src="/theme_mobicraft/static/src/js/ion.rangeSlider.min.js" type="text/javascript"/>
                <!-- Custom -->
                <!-- Theme -->
                <link href="/theme_mobicraft/static/src/less/options/colors/colors.less" rel="stylesheet" type="text/less"/>
                <link href="/theme_mobicraft/static/src/less/mixins.less" rel="stylesheet" type="text/less"/>
                <link href="/theme_mobicraft/static/src/less/box-layout.less" rel="stylesheet" type="text/less"/>
                <link href="/theme_mobicraft/static/src/less/header.less" rel="stylesheet" type="text/less"/>
                <link href="/theme_mobicraft/static/src/less/footer.less" rel="stylesheet" type="text/less"/>
                <link href="/theme_mobicraft/static/src/less/product.less" rel="stylesheet" type="text/less"/>
                <link href="/theme_mobicraft/static/src/less/style.less" rel="stylesheet" type="text/less"/>
                <link href="/theme_mobicraft/static/src/less/web.less" rel="stylesheet" type="text/less"/>
                <link href="/theme_mobicraft/static/src/css/style.css" rel="stylesheet" type="text/less"/>
                <script src="/theme_mobicraft/static/src/js/owl.carousel.min.js" type="text/javascript"/>
                <!-- <script type="text/javascript" src="/theme_mobicraft/static/src/js/carousel-swipe.js"></script>-->
                <script src="/theme_mobicraft/static/src/js/theme_mobicraft_frontend.js" type="text/javascript"/>
                <script src="/theme_mobicraft/static/src/js/theme_mobicraft.js" type="text/javascript"/>
                <script src="/theme_mobicraft/static/src/js/custom.js" type="text/javascript"/>

                <script src="/theme_mobicraft/static/src/js/rating_state.js" type="text/javascript"/>


            </xpath>
        </template>
        <!-- Assets Editor -->
        <template id="assets_editor" inherit_id="web_editor.assets_editor" name="Theme Mobicraft assets editor">
            <xpath expr="." position="inside">
                <script src="/theme_mobicraft/static/src/js/theme_mobicraft_editor.js" type="text/javascript"/>
            </xpath>
        </template>
    </data>
</odoo>